<resources xmlns:android="http://schemas.android.com/apk/res/android">

	<style name="AppBaseTheme" parent="android:Theme.Light"></style>

	<style name="AppTheme" parent="AppBaseTheme"></style>

	<style name="WTM_Main"></style>

	<style name="WTM_Main_Activity" parent="WTM_Main">
		<item name="android:background">#31353e</item>
		<item name="android:textColor">#ffffff</item>
	</style>

	<style name="WTM_Main_Button" parent="WTM_Main_Activity">
		<item name="android:background">@drawable/btn_gray</item>
		<item name="android:shadowColor">#3a4b7c</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#000000</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="WTM_Main_Button_Login" parent="WTM_Main_Activity">
		<item name="android:background">@drawable/btn_fb_blue</item>
		<item name="android:shadowColor">#3a4b7c</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="WTM_Main_Button_Login_Cancel" parent="WTM_Main_Activity">
		<item name="android:background">@drawable/btn_red</item>
		<item name="android:shadowColor">#3a4b7c</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="WTM_User_Room_Info_Title">
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textSize">20dp</item>
		<item name="android:textStyle">bold</item>
	</style>

	<style name="WTM_User_Room_Info_Desc">
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
	</style>

	<style name="WTM_User_Room_Info_Manager_Name">
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
		<item name="android:textSize">30dp</item>
	</style>

	<style name="WTM_User_Room_Info_Checked_Label">
		<item name="android:shadowColor">#000000</item>
		<item name="android:shadowDx">1</item>
		<item name="android:shadowDy">1</item>
		<item name="android:shadowRadius">2</item>
		<item name="android:textColor">#ffffff</item>
	</style>

	<style name="WTM_CategoryRoomListLabel">
		<item name="android:textSize">15dp</item>
	</style>

	<style name="WTM_Label">
		<item name="android:textSize">11dp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">#0099cc</item>
		<item name="android:paddingBottom">3dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>

	<style name="WTM_LabelBar">
		<item name="android:height">1px</item>
		<item name="android:background">#9ae2fc</item>
	</style>

	<style name="WTM_Category_Room_List_Title">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">18dp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">10dp</item>
	</style>

	<style name=" customDialog ">
		<item name="android:windowFrame">@null</item>
		<item name="android:windowBackground">@android:color/transparent</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:windowContentOverlay">@null</item>
		<item name="android:windowTitleStyle">@null</item>
		<item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
		<item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
		<item name="android:backgroundDimEnabled">false</item>
		<item name="android:background">@android:color/transparent</item>
	</style>

	<style name="AppButtonGray">
		<item name="android:textColor">#000000</item>
		<item name="android:textSize">12dp</item>
		<item name="android:background">@drawable/btn_gray</item>
		<item name="android:shadowRadius">15</item>
		<item name="android:shadowColor">#55ffffff</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:paddingRight">15dp</item>
	</style>

	<style name="AppButtonBlue">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">12dp</item>
		<item name="android:background">@drawable/btn_comm_blue</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">15dp</item>
		<item name="android:paddingRight">15dp</item>
	</style>

	<style name="AppButtonDialog">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">12dp</item>
		<item name="android:background">@drawable/btn_comm_dialog</item>
	</style>

	<style name="TabButton">
		<item name="android:textColor">#0099cc</item>
		<item name="android:textSize">12dp</item>
		<item name="android:background">@drawable/btn_tab</item>
		<item name="android:shadowRadius">15</item>
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TagCategroyName">
		<item name="android:textColor">#ffffff</item>
		<item name="android:textSize">11dp</item>
		<item name="android:background">@drawable/tag_category</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>

</resources>