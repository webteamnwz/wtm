<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/activity_room_list"
	style="@style/WTM_Main_Activity"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical" >

	<ScrollView
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="10dp" >

		<LinearLayout
			android:id="@+id/layout_room_list_main"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical" >

			<TextView
				android:id="@+id/textView1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginBottom="15dp"
				android:text="가입된 방 목록"
				android:textColor="#ffffff" />

			<LinearLayout
				android:id="@+id/layout_room_list"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical" >
			</LinearLayout>

			<Button
				android:id="@+id/buttonTest"
				style="@style/WTM_Main_Button"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:text="API Test" />

			<Button
				android:id="@+id/buttonTest2"
				style="@style/WTM_Main_Button"
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
				android:layout_marginTop="20dp"
				android:text="카테고리" />
		</LinearLayout>
	</ScrollView>

</LinearLayout>