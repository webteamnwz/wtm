<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/activity_test"
	style="@style/WTM_Main_Activity"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:orientation="vertical" >

	<ScrollView
		android:id="@+id/scrollView1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content" >

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical"
			android:paddingBottom="10dp"
			android:paddingLeft="10dp"
			android:paddingRight="10dp"
			android:paddingTop="10dp" >

			<TextView
				android:id="@+id/textStatus"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Ready"
				android:textColor="#ffffff" />

			<ImageView
				android:id="@+id/imageView1"
				android:layout_width="40dp"
				android:layout_height="40dp" />

			<LinearLayout
				android:id="@+id/layoutTest"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical" >

				<Button
					android:id="@+id/buttonTest1"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - User" />

				<Button
					android:id="@+id/buttonTest2"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - Category" />

				<Button
					android:id="@+id/buttonTest3"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - User Put" />

				<Button
					android:id="@+id/buttonTest6"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - User Out" />

				<Button
					android:id="@+id/buttonTest4"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - My Room" />

				<Button
					android:id="@+id/buttonTest5"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - Join Room" />

				<Button
					android:id="@+id/buttonTest7"
					style="@style/WTM_Main_Button"
					android:layout_width="fill_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="20dp"
					android:text="API - Check Room" />
			</LinearLayout>
		</LinearLayout>
	</ScrollView>

</LinearLayout>