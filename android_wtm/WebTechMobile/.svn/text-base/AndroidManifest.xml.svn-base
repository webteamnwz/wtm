<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="kr.po1.webtechmobile"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-sdk
		android:minSdkVersion="8"
		android:targetSdkVersion="17" />

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme" >
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityMain"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityHome"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.HOME" />
			</intent-filter>
		</activity>
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityCategoryList"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.CATEGORY_LIST" />
			</intent-filter>
		</activity>
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityRoomList"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.ROOM_LIST" />
			</intent-filter>
		</activity>
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityUserRoomInfo"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.USER_ROOM_INFO" />
			</intent-filter>
		</activity>
		<activity
			android:name="kr.po1.webtechmobile.WtmActivityTest"
			android:configChanges="orientation|screenSize"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.TEST" />
			</intent-filter>
		</activity>
	</application>

</manifest>